    <p id="loading" style="display: none;">Please wait, loading Scene ...</p>

    <script>
        const socket = io('http://localhost:8000')
        let players = [];
        let myID = "";
        let the_data = {};

        socket.on('message', (data) => {
            var data = JSON.parse(data);
            the_data = data;
            console.log(the_data);
        })

        const send_now = (msg) => {
            socket.emit('message', JSON.stringify({ type: "move", content: msg }));
        }
    </script>
